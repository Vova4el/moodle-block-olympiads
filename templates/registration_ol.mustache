<div class="olympiad-card">
    <div class="olympiad-image">
        <img src="{{image}}" alt="{{name}}" />
    </div>
    <div class="olympiad-details">
        <h3 class = "title">{{name}}</h3>
        <p>{{#str}}startdate_ol, block_olympiads{{/str}} {{startdate}} {{#str}}to_ol, block_olympiads{{/str}} {{enddate}}</p>
	<p>{{description}}</p>
        <a href="{{signupurl}}" class="btn btn-primary">{{#str}}signup, block_olympiads{{/str}}</a>
    </div>
</div>

